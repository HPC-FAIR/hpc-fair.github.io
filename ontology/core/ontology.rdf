    <!-- http://www.w3.org/ns/prov#atLocation -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#atLocation">
        <owl:equivalentProperty rdf:resource="https://hpc-fair.github.io/ontology#hasLocation"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#accepts -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#accepts">
        <rdfs:comment>A software tool accepts certain programming languages</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#actedOnBehalfOf -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#actedOnBehalfOf">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#wasInfluencedBy"/>
        <rdfs:comment>Some agent acted on behalf of another agent.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#after -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#after">
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#before"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#aiModel -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#aiModel">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#data"/>
        <rdfs:comment>a project has an ai model</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#associatedSoftware -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#associatedSoftware"/>
    


    <!-- https://hpc-fair.github.io/ontology#awardInstitution -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#awardInstitution">
        <rdfs:comment>A funded project has an award insititution.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#before -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#before">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#benchmark -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#benchmark">
        <rdfs:comment>Link to the associated benchmark software ID</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#conducted -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#conducted">
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#wasConductedBy"/>
        <rdfs:comment>an agent conducted an activity</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#consistsOf -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#consistsOf"/>
    


    <!-- https://hpc-fair.github.io/ontology#contributor -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#contributor">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#wasAttributeTo"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#coprocessor -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#coprocessor">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#processor"/>
        <rdfs:comment>a computer has a coprocessor</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#correspondsToArtifactTemplate -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#correspondsToArtifactTemplate">
        <rdfs:comment>Artifact of an activity corresponds to a artifact template of a plan. Similar to opmv:correspondsToTemplateArtifact.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#correspondsToPlan -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#correspondsToPlan">
        <rdfs:comment>Some activity corresponds to (follows) a plan.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#correspondsToStep -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#correspondsToStep">
        <rdfs:comment>Some activity corresponds to a step of a plan. Similar to ompw:correspondsToTemplateProcess.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#cpu -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#cpu">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#processor"/>
        <rdfs:comment>a computer has a cpu object linked</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#creator -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#creator">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#contributor"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#data -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#data">
        <rdfs:comment>a project has data (dataset, aiModel ,etc). Derived from experiment generating or using the data.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#dataset -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#dataset">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#data"/>
        <rdfs:comment>a project has a dataset</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#feed -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#feed">
        <rdfs:comment>link to software which use this data as input: inverse of hpc:input or hpc:use</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#file -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#file">
        <rdfs:range rdf:resource="https://hpc-fair.github.io/ontology#File"/>
        <rdfs:comment>a data object has an associated file</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#fundedBy -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#fundedBy">
        <rdfs:comment>a project is funded by some agencies</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#generated -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#generated">
        <owl:equivalentProperty rdf:resource="https://hpc-fair.github.io/ontology#hadOutput"/>
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#wasGeneratedBy"/>
        <rdfs:comment>Some activie generated some artifact</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#generates -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#generates">
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#isGeneratedBy"/>
        <rdfs:comment>Some activity/experiment generates some data (workflow, dataset, ai models, etc. )</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#graphLink -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#graphLink">
        <rdfs:comment>the top level object properties for any links between AST nodes</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hadInput -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hadInput">
        <owl:equivalentProperty rdf:resource="https://hpc-fair.github.io/ontology#used"/>
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#wasUsedBy"/>
        <rdfs:comment>Link to ids of input datasets used to generate this model (dataset)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hadMember -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hadMember">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#wasInfluencedBy"/>
        <rdfs:comment>A collection had some member entity(thing).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hadOutput -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hadOutput">
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#wasGeneratedBy"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hadPrimarySource -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hadPrimarySource">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#wasDerivedFrom"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hasCompiler -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasCompiler"/>
    


    <!-- https://hpc-fair.github.io/ontology#hasInput -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasInput">
        <owl:equivalentProperty rdf:resource="https://hpc-fair.github.io/ontology#uses"/>
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#isInputOf"/>
        <rdfs:range rdf:resource="https://hpc-fair.github.io/ontology#ArtifactTemplate"/>
        <rdfs:comment>A plan or step has some input artifact template</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hasLocation -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasLocation">
        <rdfs:range rdf:resource="https://hpc-fair.github.io/ontology#Location"/>
        <rdfs:comment>The location information of a computer or an organization</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hasMember -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasMember">
        <owl:equivalentProperty rdf:resource="https://hpc-fair.github.io/ontology#member"/>
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#memberOf"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hasNode -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasNode">
        <rdfs:comment>Link a cluster to its login or compute nodes.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hasOperatingSystem -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasOperatingSystem">
        <owl:equivalentProperty rdf:resource="https://hpc-fair.github.io/ontology#operatingSystem"/>
        <rdfs:comment>Link from a machine to OS software ID: RHEL version 8, the one runs on the computer</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hasOutput -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasOutput">
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#isOutputOf"/>
        <rdfs:range rdf:resource="https://hpc-fair.github.io/ontology#ArtifactTemplate"/>
        <rdfs:comment>A plan or step has some artifact template</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#hasPI -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasPI"/>
    


    <!-- https://hpc-fair.github.io/ontology#hasSize -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasSize"/>
    


    <!-- https://hpc-fair.github.io/ontology#hasVendor -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#hasVendor"/>
    


    <!-- https://hpc-fair.github.io/ontology#invalidated -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#invalidated">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#wasInvalidatedBy"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isArtifactTemplateOfPlan -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isArtifactTemplateOfPlan">
        <rdfs:comment>some artifact template belongs to a plan as input or output. simiar to opmw:isVariableOfTemplate</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isAssociatedWith -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isAssociatedWith">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#isInfluencedBy"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isAssociatedWithHardware -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isAssociatedWithHardware">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#isAssociatedWith"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isAssociatedWithSoftware -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isAssociatedWithSoftware">
        <rdfs:subPropertyOf rdf:resource="https://hpc-fair.github.io/ontology#isAssociatedWith"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isDecomposedAsPlan -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isDecomposedAsPlan">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:comment>MultiStep is decomposed to a Plan</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isDerivedFrom -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isDerivedFrom">
        <rdfs:comment>some artifact template is derived from another artifact template, after some steps.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isGeneratedBy -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isGeneratedBy">
        <owl:equivalentProperty rdf:resource="https://hpc-fair.github.io/ontology#isOutputOf"/>
        <rdfs:domain rdf:resource="https://hpc-fair.github.io/ontology#ArtifactTemplate"/>
        <rdfs:comment>Some artifact template is generated by a plan or step</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isInfluencedBy -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isInfluencedBy"/>
    


    <!-- https://hpc-fair.github.io/ontology#isInputOf -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isInputOf">
        <rdfs:comment>some artifact template is an input of a plan or step</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isOutputOf -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isOutputOf">
        <rdfs:comment>some artifact template is output of a plan or step</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isPartOf -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isPartOf">
        <rdfs:comment>is a part of bigger organization entities, used to support parent-child organization relation</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isPreceededBy -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isPreceededBy">
        <rdfs:domain rdf:resource="https://hpc-fair.github.io/ontology#Step"/>
        <rdfs:range rdf:resource="https://hpc-fair.github.io/ontology#Step"/>
        <rdfs:comment>A step is preceeded by another step.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isStepOfPlan -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isStepOfPlan">
        <rdfs:domain rdf:resource="https://hpc-fair.github.io/ontology#Step"/>
        <rdfs:range rdf:resource="https://hpc-fair.github.io/ontology#Plan"/>
        <rdfs:comment>a step is a step of a plan. Similar to opmw:isStepOfTemplate</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isSubPlanOfPlan -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isSubPlanOfPlan">
        <rdfs:comment>A plan is a sub plan of another plan.  Linking MultiStep to its parent Plan.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#isVariableTemplateOfPlan -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#isVariableTemplateOfPlan">
        <rdfs:comment>similar to opmw:isVariableOfPlan.  Links a variable template to its plan</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#member -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#member">
        <owl:inverseOf rdf:resource="https://hpc-fair.github.io/ontology#memberOf"/>
    </owl:ObjectProperty>
    


    <!-- https://hpc-fair.github.io/ontology#memberOf -->


    <owl:ObjectProperty rdf:about="https://hpc-fair.github.io/ontology#memberOf"/>
    


