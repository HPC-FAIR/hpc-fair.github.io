#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/ns/prov#atLocation
<http://www.w3.org/ns/prov#atLocation> rdf:type owl:ObjectProperty ;
                                       owl:equivalentProperty :hasLocation .


###  https://hpc-fair.github.io/ontology#accepts
:accepts rdf:type owl:ObjectProperty ;
         rdfs:comment "A software tool accepts certain programming languages" .


###  https://hpc-fair.github.io/ontology#access
:access rdf:type owl:ObjectProperty ;
        owl:inverseOf :accessedBy ;
        rdfs:comment "function relation" .


###  https://hpc-fair.github.io/ontology#accessedBy
:accessedBy rdf:type owl:ObjectProperty ;
            rdfs:comment "variable relation" .


###  https://hpc-fair.github.io/ontology#actedOnBehalfOf
:actedOnBehalfOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :wasInfluencedBy ;
                 rdfs:comment "Some agent acted on behalf of another agent." .


###  https://hpc-fair.github.io/ontology#after
:after rdf:type owl:ObjectProperty ;
       owl:inverseOf :before ;
       rdf:type owl:TransitiveProperty .


###  https://hpc-fair.github.io/ontology#aiModel
:aiModel rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :data ;
         rdfs:comment "a project has an ai model" .


###  https://hpc-fair.github.io/ontology#alias
:alias rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :graphLink ;
       <http://schema.org/domainIncludes> :Variable ;
       <http://schema.org/rangeIncludes> :Variable ;
       rdfs:comment "Indicates a variable aliases another variable within some context" .


###  https://hpc-fair.github.io/ontology#allocate
:allocate rdf:type owl:ObjectProperty ;
          owl:inverseOf :allocatedBy .


###  https://hpc-fair.github.io/ontology#allocatedBy
:allocatedBy rdf:type owl:ObjectProperty ;
             rdfs:comment "array relation" .


###  https://hpc-fair.github.io/ontology#associatedSoftware
:associatedSoftware rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#author
:author rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :creator ;
        rdfs:comment "Link a paper to one of its author" .


###  https://hpc-fair.github.io/ontology#authors
:authors rdf:type owl:ObjectProperty ;
         rdfs:comment "link to an orderded list of authors" .


###  https://hpc-fair.github.io/ontology#awardInstitution
:awardInstitution rdf:type owl:ObjectProperty ;
                  rdfs:comment "A funded project has an award insititution." .


###  https://hpc-fair.github.io/ontology#before
:before rdf:type owl:ObjectProperty ,
                 owl:TransitiveProperty .


###  https://hpc-fair.github.io/ontology#benchmark
:benchmark rdf:type owl:ObjectProperty ;
           rdfs:comment "Link to the associated benchmark software ID" .


###  https://hpc-fair.github.io/ontology#callSite
:callSite rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :graphLink ;
          owl:inverseOf :srcFunc ;
          <http://schema.org/domainIncludes> :Function ;
          <http://schema.org/rangeIncludes> :Callsite ;
          rdfs:comment """a function has one or more call sites where it calls other functions.
This property links a function to its function call sites.""" .


###  https://hpc-fair.github.io/ontology#calledBy
:calledBy rdf:type owl:ObjectProperty ;
          owl:equivalentProperty :caller ;
          rdfs:subPropertyOf :graphLink ;
          owl:inverseOf :calls ;
          rdf:comment "Associates a callee function with a caller function that invokes it." .


###  https://hpc-fair.github.io/ontology#callee
:callee rdf:type owl:ObjectProperty ;
        owl:equivalentProperty :calls ;
        rdfs:subPropertyOf :graphLink ;
        owl:inverseOf :caller .


###  https://hpc-fair.github.io/ontology#caller
:caller rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :graphLink ;
        <http://schema.org/domainIncludes> :Function ;
        <http://schema.org/rangeIncludes> :Function .


###  https://hpc-fair.github.io/ontology#calls
:calls rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :graphLink ;
       <http://schema.org/domainIncludes> :Function ;
       <http://schema.org/rangeIncludes> :Function ;
       rdfs:comment "a function calls another function" .


###  https://hpc-fair.github.io/ontology#conducted
:conducted rdf:type owl:ObjectProperty ;
           owl:inverseOf :wasConductedBy ;
           rdfs:comment "an agent conducted an activity" .


###  https://hpc-fair.github.io/ontology#consistsOf
:consistsOf rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#contributor
:contributor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :wasAttributeTo .


###  https://hpc-fair.github.io/ontology#coprocessor
:coprocessor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :processor ;
             rdfs:comment "a computer has a coprocessor" .


###  https://hpc-fair.github.io/ontology#correspondsToArtifactTemplate
:correspondsToArtifactTemplate rdf:type owl:ObjectProperty ;
                               rdfs:comment "Artifact of an activity corresponds to a artifact template of a plan. Similar to opmv:correspondsToTemplateArtifact." .


###  https://hpc-fair.github.io/ontology#correspondsToPlan
:correspondsToPlan rdf:type owl:ObjectProperty ;
                   rdfs:comment "Some activity corresponds to (follows) a plan." .


###  https://hpc-fair.github.io/ontology#correspondsToStep
:correspondsToStep rdf:type owl:ObjectProperty ;
                   rdfs:comment "Some activity corresponds to a step of a plan. Similar to ompw:correspondsToTemplateProcess." .


###  https://hpc-fair.github.io/ontology#cpu
:cpu rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf :processor ;
     rdfs:comment "a computer has a cpu object linked" .


###  https://hpc-fair.github.io/ontology#creator
:creator rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :contributor .


###  https://hpc-fair.github.io/ontology#data
:data rdf:type owl:ObjectProperty ;
      rdfs:comment "a project has data (dataset, aiModel ,etc). Derived from experiment generating or using the data." .


###  https://hpc-fair.github.io/ontology#dataset
:dataset rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :data ;
         rdfs:comment "a project has a dataset" .


###  https://hpc-fair.github.io/ontology#deallocatedBy
:deallocatedBy rdf:type owl:ObjectProperty ;
               rdfs:comment "array relation" .


###  https://hpc-fair.github.io/ontology#decisionFeature
:decisionFeature rdf:type owl:ObjectProperty ;
                 rdfs:comment "For a decision tree node, the feature used as the left operand of the decision condition." .


###  https://hpc-fair.github.io/ontology#decisionLabel
:decisionLabel rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :graphLink ;
               rdfs:comment "The final decision for a leaf node in a decisiontree" .


###  https://hpc-fair.github.io/ontology#declaredBy
:declaredBy rdf:type owl:ObjectProperty ;
            <http://schema.org/domainIncludes> :Variable ;
            <http://schema.org/rangeIncludes> :Statement ;
            rdfs:comment "a variable is declared by a statment" ,
                         "variable relation" .


###  https://hpc-fair.github.io/ontology#definedBy
:definedBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :graphLink .


###  https://hpc-fair.github.io/ontology#destFunc
:destFunc rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :graphLink ;
          owl:inverseOf :upstreamCallSite ;
          rdf:type owl:FunctionalProperty ;
          <http://schema.org/rangeIncludes> :Function ;
          rdfs:comment "Indicates the callee function of a callsite" .


###  https://hpc-fair.github.io/ontology#education
:education rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range :Education ;
           rdfs:comment "Link a person with his/her education history (Education objects)" .


###  https://hpc-fair.github.io/ontology#employment
:employment rdf:type owl:ObjectProperty ;
            rdfs:domain :Person ;
            rdfs:range :Employment ;
            rdfs:comment "Link a person with his/her employment history (Employment objects)" .


###  https://hpc-fair.github.io/ontology#falseNode
:falseNode rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :graphLink ;
           rdfs:comment "link from a decision tree node to its false child node" .


###  https://hpc-fair.github.io/ontology#feed
:feed rdf:type owl:ObjectProperty ;
      rdfs:comment "link to software which use this data as input: inverse of hpc:input or hpc:use" .


###  https://hpc-fair.github.io/ontology#file
:file rdf:type owl:ObjectProperty ;
      rdfs:range :File ;
      rdfs:comment "a data object has an associated file" .


###  https://hpc-fair.github.io/ontology#function
:function rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :graphLink ;
          rdfs:comment "a call graph (or other program analysis info object) has a function node." .


###  https://hpc-fair.github.io/ontology#fundedBy
:fundedBy rdf:type owl:ObjectProperty ;
          rdfs:comment "a project is funded by some agencies" .


###  https://hpc-fair.github.io/ontology#generated
:generated rdf:type owl:ObjectProperty ;
           owl:equivalentProperty :hadOutput ;
           owl:inverseOf :wasGeneratedBy ;
           rdfs:comment "Some activie generated some artifact" .


###  https://hpc-fair.github.io/ontology#generates
:generates rdf:type owl:ObjectProperty ;
           owl:inverseOf :isGeneratedBy ;
           rdfs:comment "Some activity/experiment generates some data (workflow, dataset, ai models, etc. )" .


###  https://hpc-fair.github.io/ontology#graphLink
:graphLink rdf:type owl:ObjectProperty ;
